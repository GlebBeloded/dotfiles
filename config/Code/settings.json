{
    "diffEditor.ignoreTrimWhitespace": false,
    "workbench.editor.enablePreview": false,
    "window.zoomLevel": 0,
    "go.formatTool": "gofmt",
    "workbench.editor.showTabs": true,
    "editor.lineNumbers": "relative",
    "window.menuBarVisibility": "toggle",
    "window.enableMenuBarMnemonics": true,
    "keyboard.dispatch": "keyCode",
    "editor.renderWhitespace": "all",
    "git.autofetch": true,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "editor.fontLigatures": true,
    "editor.fontFamily": "'Cascadia',monospace,'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "go.coverOnSingleTestFile": true,
    "go.lintOnSave": "workspace",
    "go.useCodeSnippetsOnFunctionSuggest": true,
    "go.coverOnSingleTest": true,
    "vscode-neovim.neovimPath": "/usr/bin/nvim",
    "editor.rulers": [
        119
    ],
    "workbench.colorCustomizations": {
        "editorRuler.foreground": "#8357a8",
    },
    "cSpell.userWords": [
        "Beloded",
        "Debugf",
        "Debugln",
        "Dotfiles",
        "Errorln",
        "Gleb",
        "Infof",
        "Infoln",
        "Luint",
        "Traceln",
        "Warningln",
        "abci",
        "amqp",
        "cmake",
        "cppcheck",
        "crossplatform",
        "devel",
        "gaiacli",
        "graphql",
        "hasura",
        "isready",
        "mapofzones",
        "mutexed",
        "opengl",
        "pidof",
        "postgres",
        "rabbitmq",
        "rabbitmqctl",
        "sigc",
        "struct"
    ],
    "go.buildOnSave": "off",
    "go.installDependenciesWhenBuilding": false,
    "go.lintTool": "golangci-lint",
    "go.vetOnSave": "off",
    "go.useLanguageServer": true,
    "cmake.configureOnOpen": true,
    "C_Cpp.clang_format_fallbackStyle": "LLVM",
    "extensions.ignoreRecommendations": false,
    "vscode-neovim.useCtrlKeysForNormalMode": false,
    "telemetry.enableTelemetry": false,
    "explorer.confirmDelete": false,
    "C_Cpp.updateChannel": "Insiders",
    "workbench.colorTheme": "Spacemacs - dark"
